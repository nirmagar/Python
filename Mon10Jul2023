#3.6.1.9 LAB
def remove_duplicates(input_list):
    result = []  # Create an empty list to store unique numbers
    for num in input_list:  # Iterate through each number in the input list
        if num not in result:  # Check if the number is not already in the result list
            result.append(num)  # If not, add it to the result list
    return result  # Return the list with duplicate numbers removed

# Example usage
numbers = [1, 2, 3, 4, 2, 5, 3, 6, 1]
unique_numbers = remove_duplicates(numbers)
print(unique_numbers)

